{
	"name": "mediasoup-svelte",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"dev": "cross-env NODE_ENV=development node server.js",
		"build": "vite build",
		"start": "node server.js",
		"prepare": "svelte-kit sync || echo ''",
